public class Food
{
    public virtual void Eat()=>Console.WriteLine("Едим...");

}
public class Pizza: Food
{
    public override void Eat()=>Console.WriteLine("Едим шаурму!");
}
public class Burger : Food
{
    public override void Eat()=>Console.WriteLine("Едим чебурек!");
}
 class Program
 {
     static void Main(string[] args)
     {
         Food[] lunch = { new Pizza(), new Burger(), new Food() };
         foreach (var f in lunch) f.Eat();
         Console.ReadLine();
     }
 }
